  "scripts": {
    "dev": "rimraf dist && nodemon -e ts --exec \"npm run start\"",
    "start": "npm run build && node --max-old-space-size=4096 dist/main.cjs",
    "build": "rimraf dist && rsbuild build",
    "build:worker": "rimraf dist && rsbuild build --config rsbuild.worker.config.ts",
    "worker": "npm run build:worker && node --max-old-space-size=4096 dist/worker.cjs",
    "worker:dev": "tsx watch src/worker.ts",
    "preview": "rsbuild preview",
    "type-check": "tsc --noEmit",
    "migration:generate": "drizzle-kit generate",
    "migration:migrate": "drizzle-kit migrate",
    "migration:push": "drizzle-kit push",
    "migration:pull": "drizzle-kit pull",
    "migration:check": "drizzle-kit check",
    "migration:up": "drizzle-kit up",
    "migration:studio": "drizzle-kit studio",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
